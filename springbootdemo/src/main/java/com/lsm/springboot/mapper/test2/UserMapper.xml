<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lsm.springboot.mapper.test2.UserMapper" >
  <resultMap id="BaseResultMap" type="com.lsm.springboot.domain.User" >
    <id column="id" property="id" jdbcType="BIGINT" />
    <result column="phoneNum" property="phonenum" jdbcType="BIGINT" />
    <result column="name" property="name" jdbcType="VARCHAR" />
    <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
  </resultMap>

  <sql id="Base_Column_List" >
    id, phoneNum, name, create_time
  </sql>

  <insert id="insert" parameterType="com.lsm.springboot.domain.User" useGeneratedKeys="true" keyProperty="id">
    insert into t_user (id, phoneNum, name, create_time)
    values (#{id,jdbcType=BIGINT}, #{phonenum,jdbcType=BIGINT}, #{name,jdbcType=VARCHAR}, #{createTime,jdbcType=TIMESTAMP})
  </insert>

  <select id="selectByPrimaryKey" resultMap="BaseResultMap">
    SELECT
    <include refid="Base_Column_List" />
    FROM t_user
    WHERE id=#{id}
  </select>

  <select id="selectByPhoneNum" resultMap="BaseResultMap">
    SELECT
    <include refid="Base_Column_List" />
    FROM t_user
    WHERE phoneNum=#{phoneNum}
  </select>

</mapper>